{
	"name": "test-ssr-vue",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "node ssr-dev",
		"prod": "npm run build && node ssr-prod",
		"view": "serve ./dist/static -p 12345",
		"gen": "vite build --ssrManifest --outDir dist/static/test && npm run build:server && node generate",
		"build": "npm run build:client && npm run build:server",
		"build:noExternal": "npm run build:client && npm run build:server:noExternal",
		"build:client": "vite build --ssrManifest --outDir dist/client",
		"build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
		"build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.js --outDir dist/server",
		"debug": "node --inspect-brk server"
	},
	"dependencies": {
		"@nuxt/devalue": "^2.0.0",
		"@vitejs/plugin-vue": "^3.1.0",
		"@vitejs/plugin-vue-jsx": "^2.0.1",
		"@vueuse/core": "^9.2.0",
		"axios": "^0.27.2",
		"example-external-component": "file:example-external-component",
		"localforage": "^1.10.0",
		"pinia": "^2.0.18",
		"serve": "^14.0.1",
		"vite": "^3.1.0",
		"vue": "^3.2.37",
		"vue-router": "^4.1.5",
		"vue-server-renderer": "^2.6.14",
		"vuex": "^4.0.2"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^3.1.9",
		"adm-zip": "^0.5.9",
		"clean-css": "^5.3.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dep-import-type": "link:./dep-import-type",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"fs-extra": "^10.1.0",
		"node-fetch": "^2.6.1",
		"serve-static": "^1.15.0",
		"uglify-js": "^3.16.2",
		"unplugin-auto-import": "^0.4.20",
		"unplugin-vue-components": "^0.17.9",
		"vite-plugin-restart": "^0.0.2"
	}
}
